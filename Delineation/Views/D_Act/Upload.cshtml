@model Delineation.Models.D_Act;
@{
    ViewData["Title"] = "Загрузка файла";
}

    <h1>Загрузка файла</h1>
    <h3>id:@Model.Id</h3>
    <h2>path:@ViewBag.xx</h2>

@using (Html.BeginForm("Upload", "D_Act", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="hidden" name="id" value="@Model.Id" />
    <input type="file" name="postedFiles" />
    <br />
    <span style="color:green">@Html.Raw(ViewBag.Message)</span>
    <br />
    <input type="submit" value="Загрузить" />
}